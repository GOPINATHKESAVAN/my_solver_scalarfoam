FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      s;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 400;

boundaryField
{
    movingWall
    {
        type            fixedValue;
	    value		    uniform 500;
    }

    fixedWalls
    {
        type            fixedValue;
	    value		    uniform 500;
    };
    

    frontAndBack
    {
        type            empty;
    }
}
